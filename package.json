{
		"name": "nestjs-auth",
		"version": "0.0.1",
		"description": "",
		"author": "",
		"private": true,
		"license": "UNLICENSED",
		"scripts": {
				"prebuild": "rimraf dist",
				"build": "nest build",
				"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
				"start": "nodemon",
				"start:dev": "nest start --watch",
				"start:debug": "nest start --debug --watch",
				"start:prod": "node dist/main",
				"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
				"test": "jest",
				"test:watch": "jest --watch",
				"test:cov": "jest --coverage",
				"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
				"test:e2e": "jest --config ./test/jest-e2e.json",
				"typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/ormconfig.ts",
				"db:drop": "npm run typeorm schema:drop",
				"db:create": "npm run typeorm migration:generate -- -n",
				"db:migrate": "npm run typeorm migration:run",
				"seed:run": "npm run typeorm seed"
		},
		"dependencies": {
				"@automapper/classes": "^3.5.2",
				"@automapper/core": "^3.5.2",
				"@hapi/joi": "^17.1.1",
				"@nartc/automapper": "^7.0.4",
				"@nestjs/common": "^7.6.15",
				"@nestjs/config": "^0.6.3",
				"@nestjs/core": "^7.6.15",
				"@nestjs/jwt": "^7.2.0",
				"@nestjs/passport": "^7.1.6",
				"@nestjs/platform-express": "^7.6.15",
				"@nestjs/swagger": "^4.8.0",
				"@nestjs/typeorm": "^7.1.5",
				"@types/bcrypt": "^5.0.0",
				"@types/hapi__joi": "^17.1.7",
				"bcrypt": "^5.0.1",
				"class-transformer": "^0.4.0",
				"class-validator": "^0.13.1",
				"cors": "^2.8.5",
				"csurf": "^1.11.0",
				"dotenv": "^8.2.0",
				"faker": "^5.5.3",
				"helmet": "^4.6.0",
				"jsonwebtoken": "^8.5.1",
				"module-alias": "^2.2.2",
				"moment": "^2.29.1",
				"nestjs-typeorm-paginate": "^2.4.1",
				"nestjsx-automapper": "^3.1.4",
				"nodemon": "^2.0.12",
				"passport": "^0.4.1",
				"passport-jwt": "^4.0.0",
				"passport-local": "^1.0.0",
				"pg": "^8.6.0",
				"reflect-metadata": "^0.1.13",
				"rimraf": "^3.0.2",
				"rxjs": "^6.6.6",
				"swagger-ui-express": "^4.1.6",
				"typeorm": "^0.2.32"
		},
		"devDependencies": {
				"@automapper/types": "^3.5.2",
				"@nestjs/cli": "^7.6.0",
				"@nestjs/schematics": "^7.3.0",
				"@nestjs/testing": "^7.6.15",
				"@types/express": "^4.17.11",
				"@types/faker": "^5.5.7",
				"@types/jest": "^26.0.22",
				"@types/node": "^14.17.9",
				"@types/passport-local": "^1.0.34",
				"@types/supertest": "^2.0.10",
				"@typescript-eslint/eslint-plugin": "^4.19.0",
				"@typescript-eslint/parser": "^4.19.0",
				"eslint": "^7.22.0",
				"eslint-config-prettier": "^8.1.0",
				"eslint-plugin-prettier": "^3.3.1",
				"jest": "^26.6.3",
				"prettier": "^2.2.1",
				"supertest": "^6.1.3",
				"ts-jest": "^26.5.4",
				"ts-loader": "^8.0.18",
				"ts-node": "^9.1.1",
				"tsconfig-paths": "^3.9.0",
				"tslib": "^2.3.0",
				"typeorm-seeding": "^1.6.1",
				"typescript": "^4.2.3"
		},
		"jest": {
				"moduleFileExtensions": [
						"js",
						"json",
						"ts"
				],
				"rootDir": "src",
				"testRegex": ".*\\.spec\\.ts$",
				"transform": {
						"^.+\\.(t|j)s$": "ts-jest"
				},
				"collectCoverageFrom": [
						"**/*.(t|j)s"
				],
				"coverageDirectory": "../coverage",
				"testEnvironment": "node"
		},
		"_moduleAliases": {
				"@app": "./dist"
		}
}
